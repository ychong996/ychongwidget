<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ychong.ychongwidget">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ui.LoadingDialogActivity" />
        <activity android:name=".ui.InputDialogActivity" />
        <activity android:name=".ui.MessageDialogActivity" />
        <activity android:name=".ui.TipsDialogActivity" />
        <activity android:name=".ui.ResultDialogActivity" />
        <activity android:name=".ui.ToastDialogActivity" />
        <activity android:name=".ui.BottomListDialogActivity" />
        <activity android:name=".ui.ImageViewActivity" />
        <activity android:name=".ui.WheelPickerActivity" />
        <activity android:name=".ui.YCHWheelPickerActivity" />
    </application>

</manifest>